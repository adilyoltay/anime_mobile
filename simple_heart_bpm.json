  "stateMachines": [
    {
      "name": "HeartBPMController",
      "inputs": [
        {
          "name": "bpm",
          "type": "number",
          "defaultValue": 72,
          "minValue": 40,
          "maxValue": 180,
          "description": "Beats per minute"
        },
        {
          "name": "isActive",
          "type": "boolean",
          "defaultValue": true,
          "description": "Start/stop heart animation"
        },
        {
          "name": "intensity",
          "type": "number",
          "defaultValue": 1.0,
          "minValue": 0.5,
          "maxValue": 2.0,
          "description": "Animation intensity multiplier"
        }
      ],
      "states": [
        {
          "name": "Idle",
          "isDefault": true,
          "animations": []
        },
        {
          "name": "Beating",
          "animations": ["heartbeat", "pulse_effect"]
        },
        {
          "name": "FastBeat",
          "animations": ["heartbeat_fast", "pulse_effect"]
        }
      ],
      "transitions": [
        {
          "from": "Idle",
          "to": "Beating",
          "condition": {
            "input": "isActive",
            "value": true,
            "operator": "=="
          }
        },
        {
          "from": "Beating",
          "to": "Idle",
          "condition": {
            "input": "isActive",
            "value": false,
            "operator": "=="
          }
        },
        {
          "from": "Beating",
          "to": "FastBeat",
          "condition": {
            "input": "bpm",
            "value": 100,
            "operator": ">"
          }
        },
        {
          "from": "FastBeat",
          "to": "Beating",
          "condition": {
            "input": "bpm",
            "value": 100,
            "operator": "<="
          }
        }
      ]
    }
  ],
