      "text": "72 BPM",
      "fontSize": 16,
      "fontFamily": "Arial",
      "color": "#ffffff",
      "align": "center"
    },
    {
      "type": "ellipse",
      "name": "pulse_ring",
      "id": 4,
      "x": 200,
      "y": 200,
      "width": 150,
      "height": 150,
      "fill": {
        "type": "solid",
        "color": "#ff6b6b",
        "opacity": 0.3
      },
      "stroke": {
        "color": "#ff6b6b",
        "thickness": 2,
        "opacity": 0.6
      }
    }
  ],
  "stateMachines": [
    {
      "name": "BPMController",
      "inputs": [
        {
          "name": "bpm",
          "type": "number",
          "defaultValue": 72,
          "minValue": 40,
          "maxValue": 180
        },
        {
          "name": "isActive",
          "type": "boolean",
          "defaultValue": true
        }
      ],
      "states": [
        {
          "name": "Idle",
          "isDefault": true,
          "animations": []
        },
        {
          "name": "Beating",
          "animations": ["heartbeat"]
        }
      ],
      "transitions": [
        {
          "from": "Idle",
          "to": "Beating",
          "condition": {
            "input": "isActive",
            "value": true
          }
        },
        {
          "from": "Beating",
          "to": "Idle",
          "condition": {
            "input": "isActive",
            "value": false
          }
        }
      ]
    }
  ],
  "animations": [
    {
      "name": "heartbeat",
      "duration": 0.83,
      "workArea": {
        "start": 0,
        "end": 0.83
      },
      "fps": 60,
